{
    "name": "store",
    "version": "1.0.0",
    "description": "Store made withe express js",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon index.js",
        "start": "node index.js",
        "lint": "eslint",
        "migration:create": "sequelize-cli migration:generate --name",
        "migration:run": "sequelize-cli db:migrate",
        "migration:revert": "sequelize-cli db:migrate:undo",
        "migration:delete": "sequelize-cli db:migrate:undo:all",
        "migration:rerun": "sequelize-cli db:migrate:undo:all;npm run migration:run"
    },
    "keywords": [
        "node js",
        "express js",
        "javascript",
        "postgres",
        "sequelize"
    ],
    "author": "Ricardo Rito Anguiano <26richardr@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.1",
        "nodemon": "^2.0.13",
        "prettier": "^2.3.2"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.4",
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "faker": "^5.5.3",
        "joi": "^17.4.2",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.3.2",
        "passport": "^0.5.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.7.1",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.7.0",
        "sequelize-cli": "^5.0.0"
    },
    "engines": {
        "node": "16.x"
    }
}
